{
  "logging":{
    "path": "log/debug.log"
  },
  "probe-trigger":{
    "default-probe":"-O -sT -sV",
    "test-probe":"-sn"
  },
  "resultd":{
    "recv-channel":{
      "name":"/topic/ops-results",
      "num-connect-retries":"120",
      "time-between-connect-retries-in-seconds":"1"
    },
    "send-channel":{
      "name":"/topic/rtkbox-ops",
      "num-connect-retries":"120",
      "time-between-connect-retries-in-seconds":"1"
    }
  },
  "sensord":{
    "recv-channel":{
      "name":"/topic/peernode-ops",
      "num-connect-retries":"120",
      "time-between-connect-retries-in-seconds":"1"
    },
    "send-channel":{
      "name":"/topic/ops-results",
      "num-connect-retries":"120",
      "time-between-connect-retries-in-seconds":"1"
    },
    "sensors":{
      "pcap-sensor":{},
      "generic-sensor":{},
      "flow-sensor":{
        "applabel-rules":"etc/yafApplabelRules.conf",
        "dpi-plugin":"lib/yaf/dpacketplugin.so",
        "dpi-plugin-opts":"80",
        "dpi-rules":"etc/yafDPIRules.conf"
      }
    }
  }
}
